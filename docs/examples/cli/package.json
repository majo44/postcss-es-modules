{
    "name": "postcss-es-modules-example-cli",
    "version": "1.0.1",
    "private": true,
    "description": "Example of usage postcss-es-modules with the postcss cli",
    "license": "MIT",
    "scripts": {
        "build": "yarn test",
        "start": "node -r postcss-node/register src/index.js",
        "test": "yarn start"
    },
    "devDependencies": {
        "postcss": "^8.2.13",
        "postcss-cli": "^8.3.1",
        "postcss-es-modules": "2.0.2",
        "postcss-node": "2.0.1",
        "css-es-modules": "1.0.2",
        "typescript": "^4.1.5",
        "ts-node": "^9.1.1"
    },
    "postcss": {
        "plugins": {
            "postcss-es-modules": {
                "inject": {
                    "moduleType": "cjs"
                }
            }
        }
    }
}
